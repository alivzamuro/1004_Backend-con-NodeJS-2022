### Insert a user
POST http://localhost:4000/ HTTP/1.1
Content-Type: application/json
X-Request-Type: GraphQL

mutation {
  signUp(
    input: {
      name: "Beto"
      lastname: "Bedu"
      email: "beto@bedu.org"
      password: "b3T0P4$$w0rD"
      isAdmin: true
    }
  ) {
    id
    name
    email
  }
}

